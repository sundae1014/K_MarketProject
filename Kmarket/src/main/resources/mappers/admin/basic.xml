<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.kmarket.mapper.admin.BasicMapper">
    <select id="selectRecentVersion">
        select version_no
        from version
        order by id desc
        fetch first 1 rows only
    </select>


</mapper>